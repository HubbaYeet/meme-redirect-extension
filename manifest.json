{
  "manifest_version": 3,
  "name": "Video Redirect Extension",
  "version": "1.0",
  "description": "Redirects configured sites to a random funny video.",
  "permissions": ["tabs", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "web_accessible_resources": [
    {
      "resources": ["sigma.mp4", "bald.mp4"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  }
}